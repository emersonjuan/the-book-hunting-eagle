<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/imgBooks3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/logout"
    android:contentDescription="@string/imgBooks3"
    tools:context=".LogoutPage">

    <TextView
        android:id="@+id/txtLogout"
        android:layout_width="356dp"
        android:layout_height="78dp"
        android:gravity="center"
        android:text="@string/txtLogout"
        android:textColor="@color/material_dynamic_primary20"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.709"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.027"
        tools:ignore="TextSizeCheck" />

    <ImageView
        android:id="@+id/imgBooksLogout"
        android:layout_width="150dp"
        android:layout_height="116dp"
        android:contentDescription="@string/imgBooks3"
        android:rotationX="-3"
        android:rotationY="11"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.398"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.651"
        app:srcCompat="@drawable/books3" />
</androidx.constraintlayout.widget.ConstraintLayout>